<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Website_Cannot_Contain_Spaces</fullName>
    <active>false</active>
    <errorConditionFormula>AND(CONTAINS(Website, &apos; &apos;),
				NOT($User.ValidationRulesDoNotApply__c))</errorConditionFormula>
    <errorMessage>A website URL cannot have spaces in it</errorMessage>
</ValidationRule>
