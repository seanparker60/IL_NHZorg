<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BillingReminder_2_Cannot_Be_Same_as_BR</fullName>
    <active>true</active>
    <errorConditionFormula>AND(BillingReminderEmail2__c = BillingReminderEmail__c,NOT($User.ValidationRulesDoNotApply__c = true),NOT(ISBLANK(BillingReminderEmail__c)))</errorConditionFormula>
    <errorMessage>Billing Reminder Email 2 cannot be the same as Billing Reminder Email</errorMessage>
</ValidationRule>
