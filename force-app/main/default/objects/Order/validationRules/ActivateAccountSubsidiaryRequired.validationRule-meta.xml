<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ActivateAccountSubsidiaryRequired</fullName>
    <active>false</active>
    <errorConditionFormula>AND(TEXT(Status) = &apos;Activated&apos;,  OR(AccountSubsidiary__c = null, NetSuiteIdAccountSubsidiary__c =null))</errorConditionFormula>
    <errorDisplayField>AccountSubsidiary__c</errorDisplayField>
    <errorMessage>Actived Orders must have an Account Subsidiary.</errorMessage>
</ValidationRule>
