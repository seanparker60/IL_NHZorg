<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidateNetSalesPrice__c</fullName>
    <externalId>false</externalId>
    <formula>IF((ISPICKVAL(OrderLineType__c, &quot;Free&quot;) &amp;&amp; UnitPrice &gt; 0) || (ISPICKVAL(OrderLineType__c, &quot;Service&quot;) &amp;&amp; UnitPrice &gt; 0),1,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Validate NetSalesPrice</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
