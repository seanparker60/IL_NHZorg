<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidateLineItemTotal__c</fullName>
    <externalId>false</externalId>
    <formula>IF( AND( OrderImportLineTotal__c &gt; 0 ,
         LineItemTotal__c !=  OrderImportLineTotal__c), 
1, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Compares Line Item Total &amp; Order Import Line Total. (Currently Only Used for eCommerce Orders)</inlineHelpText>
    <label>Validate Line Item Total</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
